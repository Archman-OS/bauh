{
  "app-id": "com.github.vinifmor.bauh",
  "runtime": "org.kde.Platform",
  "runtime-version": "5.13",
  "sdk": "org.kde.Sdk",
  "command": "bauh",
  "finish-args": [
    "--socket=wayland",
    "--socket=x11",
    "--device=dri",
    "--filesystem=home",
    "--filesystem=/usr/bin",
    "--share=network",
    "--socket=system-bus",
    "--socket=session-bus"
  ],
  "modules": [
    {
      "name": "copy-icons",
      "buildsystem": "simple",
      "build-commands": [
        "install -Dm0644 32x32.png /app/share/icons/hicolor/32x32/apps/com.github.vinifmor.bauh.png",
        "install -Dm0644 48x48.png /app/share/icons/hicolor/48x48/apps/com.github.vinifmor.bauh.png",
        "install -Dm0644 48x48.png /app/share/icons/hicolor/64x64/apps/com.github.vinifmor.bauh.png"
      ],
      "sources": [
        {
          "type": "file",
          "path": "../aur/icons/32x32.png"
        },
        {
          "type": "file",
          "path": "../aur/icons/48x48.png"
        },
        {
          "type": "file",
          "path": "../aur/icons/64x64.png"
        }
      ]
    },
    {
      "name": "desktop-entry",
      "buildsystem": "simple",
      "build-commands": [
        "install -D com.github.vinifmor.bauh.desktop /app/share/applications/com.github.vinifmor.bauh.desktop"
      ],
      "sources": [
        {
          "type": "file",
          "path": "com.github.vinifmor.bauh.desktop"
        }
      ]
    },
